    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1",
    "pg": "^8.13.1",
    "puppeteer": "^24.2.0",
    "robots-parser": "^3.0.1",
    "rss-parser": "^3.13.0",
